<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="com.infinityappsolutions.webdesigner.plugin.actionSet">
         <menu
               label="Jetty Menu"
               id="sampleMenu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               label="&amp;Stop Jetty"
               icon="icons/sample.gif"
               class="com.infinityappsolutions.webdesigner.plugin.actions.StopJettyAction"
               tooltip="Hello, Eclipse world"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="com.infinityappsolutions.webdesigner.plugin.actions.StopJettyAction">
         </action> 
         <action
               label="&amp;Start Jetty"
               icon="icons/sample.gif"
               class="com.infinityappsolutions.webdesigner.plugin.actions.StartJettyAction"
               tooltip="Hello, Eclipse world"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="com.infinityappsolutions.webdesigner.plugin.actions.StartJettyAction">
         </action>
             <action
               label="&amp;New Project"
               icon="icons/sample.gif"
               class="com.infinityappsolutions.webdesigner.plugin.actions.NewProjectAction"
               tooltip="Hello, Eclipse world"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="com.infinityappsolutions.webdesigner.plugin.actions.NewProjectAction">
         </action>
      </actionSet>
   </extension>

</plugin>
